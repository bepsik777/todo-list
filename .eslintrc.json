{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base", 
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-multiple-empty-lines": "off",
        "no-param-reassign": ["warn", { "props": false } ],
        "no-use-before-define": ["error", {"functions": false}],
        "max-len": "off",
        "import/no-extraneous-dependencies": "off",
        "no-restricted-syntax": "off"
    }
}
